{
  "name": "probono-api",
  "version": "0.1.0",
  "description": "GraphQL API for ProBono.",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "seed": "ts-node src/seed.ts",
    "service": "docker-compose up -d",
    "deploy": "prisma deploy",
    "build": "tsc",
    "test": "echo \"No tests specified yet.\"",
    "production": "prisma deploy --env-file .env.prod",
    "lint": "tslint -c tslint.json \"src/**/*.ts\" -e \"src/generated/**/*.ts\" -t verbose -p tsconfig.json"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "cross-env": "5.2.0",
    "dotenv": "7.0.0",
    "graphql": "14.3.0",
    "graphql-middleware": "3.0.2",
    "graphql-shield": "6.0.6",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.11.7",
    "nexus-prisma": "0.3.7",
    "prisma-client-lib": "1.31.2",
    "sanitize-html": "1.20.1",
    "slug": "1.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.25",
    "@types/bcrypt": "3.0.0",
    "@types/dotenv": "6.1.1",
    "@types/graphql": "14.2.0",
    "@types/jsonwebtoken": "8.3.2",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.9",
    "@types/sanitize-html": "1.18.3",
    "@types/slug": "0.9.1",
    "@types/ws": "6.0.1",
    "prisma": "1.30.2",
    "ts-node-dev": "1.0.0-pre.39",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  }
}